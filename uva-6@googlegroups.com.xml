  <rdf:Description rdf:about="urn:uuid:1a48eb92-63ca-4e2a-aa5e-e764532b5d39">
    <rdf:type rdf:resource="http://www.w3.org/ns/oa#SpecificResource"/>
    <oa:hasSource rdf:resource="urn:uuid:5f0486f0-f7a9-4c44-83d1-8607a3d6c4c0"/>
